# Environment
NODE_ENV=development

# Server
PORT=3000

# MongoDB
MONGO_URI=mongodb://localhost:27017/chatbot-api

# Logging
LOG_LEVEL=info

# Rate Limiting
# The time window in milliseconds for rate limiting
RATE_LIMIT_WINDOW_MS=60000
# The maximum number of requests allowed within the rate limit window
RATE_LIMIT_MAX_REQUESTS=100

# Swagger
SWAGGER_API_DOCS_ROUTE=/api-docs

# Additional configurations
# Example API Keys or Secrets
# Uncomment and add your own values as needed
# API_KEY=your_api_key
# SECRET_KEY=your_secret_key
# REDIS_URL=redis://localhost:6379
